{
    "title": "YouTube Scraper Input",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
      "keywords": {
        "type": "array",
        "title": "Search Keywords",    
        "description": "Keywords to search",
        "items": { "type": "string" },
        "editor": "textfield"
      },
      "maxCount": {
        "type": "integer",
